{
    "name": "CyberShield",
    "version": "0.0.1",
    "manifest_version":2,
    "browser_action":{
        "default_popup": "popup.html",
        "default_icon": "cybershield_logo2.png"
    
    
    },
    "icons":{
        "128":"cybershield_logo2.png"

    },





    "permissions":["activeTab"],

    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["fetch.js"]
        }
      ],

    "content_security_policy": "script-src 'self' https://unpkg.com/ ; object-src 'self' "

}